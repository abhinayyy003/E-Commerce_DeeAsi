<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="https://unpkg.com/sheryjs/dist/Shery.css" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/locomotive-scroll@3.5.4/dist/locomotive-scroll.css"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/remixicon@3.5.0/fonts/remixicon.css"
      rel="stylesheet"
    />

    <style>
      * {
        margin: 0;
        padding: 0;
        font-family: Nimbus Sans L;
        box-sizing: border-box;
      }

      html,
      body {
        width: 100%;
        height: 100%;
        cursor: none;
      }
      #nav {
        position: relative;
        display: flex;
        justify-content: space-between;
        padding: 2vw 10vw;
      }

      #nleft {
        display: flex;
        gap: 30px;
      }

      #nav a {
        font-weight: 800;
        font-size: 14px;
        text-decoration: none;
        color: #000;
        cursor: none;
      }
      button {
        cursor: none;
      }
      #nav #centeratag {
        font-size: 16px;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
      }

      #main {
        width: 100%;
      }

      #motive {
        margin-top: 5vw;
        display: flex;
        justify-content: space-between;
        padding: 3vw 10vw;
      }

      #motive h2 {
        font-weight: 400;
        color: #5e5e5e;
      }

      #motive p {
        line-height: 1.7;
        letter-spacing: -0.12vw;
        position: relative;
        right: 0;
        width: 85%;
        text-align: right;
        font-size: 2.1vw;
      }

      #motive span {
        display: inline-block;
        border-radius: 100px;
        margin: 0 13px;
        margin-bottom: -12.5px;
      }

      #onespan {
        width: 80px;
        height: 50px;
        background-image: url(./images/GT_model.jpg);
        background-size: cover;
        background-position: 0% 25%;
      }

      #twospan {
        width: 80px;
        height: 50px;
        background-image: url(https://scontent.cdninstagram.com/v/t39.30808-6/397538718_17879549747964941_9210430863610859740_n.jpg?stp=dst-jpg_e15&efg=eyJ2ZW5jb2RlX3RhZyI6ImltYWdlX3VybGdlbi4xNDQweDE0NDAuc2RyLmYzMDgwOCJ9&_nc_ht=scontent.cdninstagram.com&_nc_cat=105&_nc_ohc=iLpNFlDmQPYAb58RFH0&edm=APs17CUAAAAA&ccb=7-5&ig_cache_key=MzIyNDgzODE2NjcwNDMwODYwOA%3D%3D.2-ccb7-5&oh=00_AfC43qLN2XMLSSdQDQVyLbhcsFraZcThdtTFn6jTUPnQLw&oe=6625F8C8&_nc_sid=10d13b);
        background-size: 115%;
        background-position: 175% 45%;
      }

      #threespan {
        width: 80px;
        height: 50px;
        background-image: url(./images/bali_women.jpg);
        background-size: cover;
        background-position: 0% 20%;
      }

      #banner {
        display: flex;
        align-items: center;
        width: 100%;
        margin-top: 8vw;
      }

      #bimg {
        width: 65%;
        height: 60vh;
      }

      #bimg img {
        width: 100%;
        height: 100%;
        object-position: 0% 70%;
        object-fit: cover;
      }

      #btext {
        width: 35%;
        padding: 0 3vw;
      }

      #btext h1 {
        font-size: 2.8vw;
        font-weight: 400;
        letter-spacing: -0.15vw;
        line-height: 1;
      }

      #btext p {
        font-size: 1vw;
        margin-top: 20px;
        width: 60%;
      }

      #future {
        position: relative;
        overflow: hidden;
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        width: 100%;
        height: 100vh;
        background-color: white;
      }

      #future video {
        opacity: 0;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 100%;
      }

      #ftext {
        display: flex;
        flex-direction: column;
        align-items: center;
        position: relative;
        z-index: 9999;
        background-color: transparent;
        padding: 1vw 2vw;
        border-radius: 10px;
      }

      #ftext h1 {
        letter-spacing: -0.09vw;
        font-weight: 100;
        font-size: 2vw;
      }

      #ftext button {
        padding: 0.8vw 1.7vw;
        font-size: 0.6vw;
        text-transform: capitalize;
        margin-top: 10px;
        font-weight: 500;
        border-radius: 100px;
        border-radius: white;
        border: 1px solid #5e5e5e;
        color: #6f6f6f;
      }
      #fourspan {
        width: 80px;
        height: 50px;
        background-image: url(./images/deeasi_insta_logo.png);
        background-size: cover;
        background-position: 0% 50%;
      }
    </style>
  </head>
  <body>
    <div class="main">
      <div id="home">
        <div id="nav">
          <div id="nleft">
            <a class="nlink" id="magnet" href="/">Home</a>
            <a class="nlink" id="magnet" href="/products">Products</a>
          </div>
          <a class="nlink test-xl" id="centeratag" href="#">DeeAsi</a>
          <div id="nright" class="flex">
            <% if(user){ %>
            <a
              href="/ordereditems"
              id="magnet"
              style="margin-right: 3vw; display: flex; position: relative"
            >
              <!-- <i class="ri-shopping-cart-fill"><span></span></i> -->
              <i class="ri-xbox-fill"></i>
            </a>
            <% } else{ %>
            <a
              onclick="alert('Please login to view your previous orders')"
              id="magnet"
              style="margin-right: 3vw; display: flex; position: relative"
            >
              <!-- <i class="ri-shopping-cart-fill"><span></span></i> -->
              <i class="ri-xbox-fill"></i>
            </a>
            <% } %>

            <a href="/login/federated/google"
              ><button
                style="font-weight: 800; font-size: 14px"
                id="magnet"
                type="submit"
              >
                Login <i class="ri-google-fill"></i></button
            ></a>
            <!-- <i class="ri-google-line"></i> -->
          </div>
        </div>
      </div>
      <div id="motive">
        <h2>ABOUT US</h2>
        <p>
          <span id="fourspan"></span>, is a contemporary women's clothing brand
          that wishes to fill your wardrobe with gorgeous, whimsycal garments
          <span id="onespan"></span>. We're all about embracing the beauty of
          every curve <span id="twospan"></span> and celebrating the unique
          essence of every woman.make you feel confident, fierce queen that you
          truly are, let your style shine like a constellation of stars!<span
            id="threespan"
          ></span
          >So, darling, are you ready to unleash your inner fashionista?
          <!-- make you feel confident, fierce queen that you truly are, because you deserve nothing less! -->
        </p>
      </div>
      <img src="./images/aboutus2.png" alt="" />

      <div id="banner">
        <div id="bimg">
          <img src="./images/deeasilogo_2.png" alt="" />
          <!-- <img src="./images/dress.png" alt="" /> -->
        </div>
        <div id="btext">
          <h1>Deepika</h1>
          <h1>Thulasi</h1>
        </div>
      </div>
    </div>

    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js"
      integrity="sha512-16esztaSRplJROstbIIdwX3N97V1+pZvV33ABoG1H2OyTttBxEGkTsoIVsiP1iaTtM8b3+hu2kB6pQ4Clr5yug=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    ></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/ScrollTrigger.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/0.155.0/three.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/automat/controlkit.js@master/bin/controlKit.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/locomotive-scroll@beta/bundled/locomotive-scroll.min.js"></script>
    <script
      type="text/javascript"
      src="https://unpkg.com/sheryjs/dist/Shery.js"
    ></script>
  </body>
  <script>
    Shery.mouseFollower({
      skew: true,
      ease: "cubic-bezier(1, 1, 1, 1)",
      duration: 0.2,
    });
    Shery.makeMagnet("#magnet", {});
    const scroll = new LocomotiveScroll({
      el: document.querySelector("#main"),
      smooth: true,
    });
    //gsap to animate
    gsap.from(".nlink", {
      stagger: 0.2,
      y: 10,
      duration: 1,
      ease: Power2,
      opacity: 0,
    });

    Shery.imageEffect("#bimg", {
      style: 5,
      config: {
        a: { value: 0.74, range: [0, 30] },
        b: { value: -0.97, range: [-1, 1] },
        zindex: { value: -9996999, range: [-9999999, 9999999] },
        aspect: { value: 1.9949048404016185 },
        gooey: { value: true },
        infiniteGooey: { value: true },
        growSize: { value: 2.74, range: [1, 15] },
        durationOut: { value: 0.55, range: [0.1, 5] },
        durationIn: { value: 1.5, range: [0.1, 5] },
        displaceAmount: { value: 0.5 },
        masker: { value: true },
        maskVal: { value: 1.2, range: [1, 5] },
        scrollType: { value: 0 },
        geoVertex: { range: [1, 64], value: 1 },
        noEffectGooey: { value: false },
        onMouse: { value: 1 },
        noise_speed: { value: 0.74, range: [0, 10] },
        metaball: { value: 0.2, range: [0, 2], _gsap: { id: 33 } },
        discard_threshold: { value: 0.5, range: [0, 1] },
        antialias_threshold: { value: 0, range: [0, 0.1] },
        noise_height: { value: 0.43, range: [0, 2] },
        noise_scale: { value: 10.74, range: [0, 100] },
      },
      gooey: true,
    });
  </script>
</html>
